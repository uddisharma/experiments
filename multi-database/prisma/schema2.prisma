generator client {
    provider = "prisma-client-js"
    output   = "./generated/client2"
}

datasource db {
    provider = "postgresql"
    url      = env("PSQL2_URL")
}

model MYSQL2 {
    id    Int    @id @default(autoincrement())
    modal String
}
